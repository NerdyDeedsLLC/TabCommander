{
  "name": "Tab Commander",
  "description": "Extract, consolidate, sort, merge, and discard tabs - in the active window or all - using text or regular expressions, all from the OmniBox!",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "omnibox",
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "TabCommander.js"
  },
  "omnibox": {
    "keyword": "tab"
  },
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon_016.png",
      "32": "icons/icon_032.png",
      "48": "icons/icon_048.png",
      "64": "icons/icon_064.png",
      "128": "icons/icon_128.png",
      "256": "icons/icon_256.png"
    }
  },
  "icons": {
    "16": "icons/icon_016.png",
    "32": "icons/icon_032.png",
    "48": "icons/icon_048.png",
    "128": "icons/icon_128.png"
  }
}
